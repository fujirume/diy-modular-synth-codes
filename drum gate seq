  A[0][0] = 1; A[0][1] = 0; A[0][2] = 0; A[0][3] = 1; A[0][4] = 0; A[0][5] = 0; A[0][6] = 1; A[0][7] = 0; 
  
  A[1][0] = 0; A[1][1] = 0; A[1][2] = 1; A[1][3] = 0; A[1][4] = 0; A[1][5] = 1; A[1][6] = 0; A[1][7] = 1;  
  
