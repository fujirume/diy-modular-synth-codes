  A[0][0] = 1; A[0][1] = 0; A[0][2] = 0; A[0][3] = 0; A[0][4] = 1; A[0][5] = 0; A[0][6] = 0; A[0][7] = 0;  A[0][8] = 1; A[0][9] = 0; A[0][10] = 0; A[0][11] = 0; A[0][12] = 1; A[0][13] = 0; A[0][14] = 0; A[0][15] = 0;  
  
  A[1][0] = 0; A[1][1] = 0; A[1][2] = 0; A[1][3] = 1; A[1][4] = 0; A[1][5] = 0; A[1][6] = 1; A[1][7] = 1;  A[1][8] = 0; A[1][9] = 0; A[1][10] = 0; A[1][11] = 1; A[1][12] = 0; A[1][13] = 0; A[1][14] = 1; A[1][15] = 1;   
  
  A[2][0] = 0; A[2][1] = 1; A[2][2] = 1; A[2][3] = 1; A[2][4] = 0; A[2][5] = 1; A[2][6] = 0; A[2][7] = 0;  A[2][8] = 0; A[2][9] = 1; A[2][10] = 1; A[2][11] = 1; A[2][12] = 0; A[2][13] = 1; A[2][14] = 0; A[2][15] = 0; 
 }

void pat1_4(){
  
  A[0][0] = 1; A[0][1] = 0; A[0][2] = 1; A[0][3] = 0; A[0][4] = 1; A[0][5] = 1; A[0][6] = 1; A[0][7] = 0;  
  
  A[1][0] = 0; A[1][1] = 0; A[1][2] = 1; A[1][3] = 0; A[1][4] = 0; A[1][5] = 0; A[1][6] = 1; A[1][7] = 0;  
  
